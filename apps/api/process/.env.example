# Database Configuration
DATABASE_URL=postgresql://username:password@localhost:5432/app_runner
DB_SSL=false

# AWS Configuration
AWS_REGION=ca-central-1
AWS_PROFILE=default

# SQS Configuration
SQS_CRAWL_QUEUE_URL=https://sqs.ca-central-1.amazonaws.com/123456789012/crawler-commands-dev
SQS_RESULTS_QUEUE_URL=https://sqs.ca-central-1.amazonaws.com/123456789012/crawler-results-dev

# SNS Configuration (optional, if using SNS instead of SQS for results)
SNS_RESULTS_TOPIC_ARN=arn:aws:sns:ca-central-1:123456789012:crawler-results-dev

# Application Configuration
PORT=3000
NODE_ENV=development

# Polling Configuration
POLLING_INTERVAL_MS=5000
MAX_CONCURRENT_CRAWLS=10
